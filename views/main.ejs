<main>
  <article class="content">
    <% if (data.category.relation === "same_categories") { %>
    <time class="published-date"><%= data.page.publishedDate %></time>
    <% } %>
    <% if (data.page.title) { %>
    <h1><%= data.page.title %></h1>
    <% } %>
    <% if (data.page.captionImage) { %>
    <img class="eyecatch-image" src="<%= data.page.captionImage %>" />
    <% } %>
    <% for (let section of data.pageContent.sections) { %>
    <%- section.html %>
    <% } %>
  </article>
</main>